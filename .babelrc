{
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src/"],
        "alias": {
          "@devHocs": "./src/dev-hocs",
          "@devTools": "./src/dev-tools",
          "@library": "./src/library",
          "@@componentEditor": "./src/dev-tools/component-editor",
          "@@notepad": "./src/dev-tools/notepad",
          "@@tasks": "./src/dev-tools/tasks",
          "@@terminal": "./src/dev-tools/terminal"
        }
      }
    ]
  ]
}
